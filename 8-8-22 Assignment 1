#include <stdio.h>

int main()
{
    int n,n1,s,r;
    printf("ENTER A NUMBER:");
    scanf("%d",&n);
    s=0;n1=n;
    while(n>0)
    {
        r=n%10;
        s=s+(r*r*r);
        n=n/10;
    }
    if(n1==s)
    printf("ARMSTRONG NUMBER");
    else
    printf("NOT ARMSTRONG NUMBER");
    
    return 0;
}
